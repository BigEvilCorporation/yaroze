CFLAGS	= -O1 -g -DPLATFORM_YAROZE
LINKER	= -Xlinker -Ttext -Xlinker 80100000
RM	= del

PROG	= main
OBJS	= \
    CDPrompt.c \
    main.c \
    Stars.c \
    framewk\\entity\\Object.c \
    framewk\\io\\CDROM.c \
    framewk\\io\\CDROM.s \
    framewk\\io\\File.c \
    framewk\\io\\Gamepad.c \
    framewk\\maths\\Maths.c \
    framewk\\render\\TIM.c \
    framewk\\render\\TMD.c \
    framewk\\render\\VRAM.c \
    framewk\\system\\Interpt.c

all: $(PROG)

$(PROG): $(OBJS)
	$(CC) $(LINKER) $(OBJS) -o $(PROG)

main.o: main.c

clean:
	$(RM) $(PROG) 
	$(RM) *.o

